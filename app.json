{
  "expo": {
    "owner": "ydreawrld",
    "name": "thegame-zagreb",
    "slug": "thegame-zagreb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoZG.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoZG.png",
      "resizeMode": "contain",
      "backgroundColor": "#31328f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_MAPS_API_KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
