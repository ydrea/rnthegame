{
  "expo": {
    "owner": "andrijavr",
    "name": "thegamezagreb",
    "slug": "thegamezagreb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoZG.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoZG.png",
      "resizeMode": "contain",
      "backgroundColor": "#31328f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
        }
      }

    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
  }
}
